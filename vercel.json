{
  "buildCommand": "npm run build:web",
  "outputDirectory": "web-build",
  "devCommand": "npm run web",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*).(js|css|woff|woff2|ttf|otf|eot|svg|png|jpg|jpeg|gif|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

